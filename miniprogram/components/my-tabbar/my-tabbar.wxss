/* components/my-tabbar/my-tabbar.wxss */
@import '../../style/animation.wxss'; 
.tabbar{
  display: flex;
  position: fixed;
  right: 0;
  left: 0;
  bottom: 0;
  height: 90rpx;
  background-color: #444;
  opacity: 0.8;
  text-align: center;
  color: #f3f3f3;
  font-size: 26rpx;
  font-weight: 100;
  box-shadow: 10rpx 10rpx 60rpx rgba(100, 100, 100, 0.8);
  animation: moveTop 0.7s forwards;
}

.tabbar view {
  flex: 1;
  width: 100%;
  height: 100%;
}
.tabbar image {
  vertical-align: middle;
  width: 48rpx;
  height: 48rpx;
  margin: 3rpx 0 1rpx;
}