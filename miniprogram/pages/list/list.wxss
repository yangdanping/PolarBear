@import '../../style/animation.wxss';
.listContainer{
  height: 4900rpx;
  background-image: var(--HomeNavBarbg);
}

.container{
  display:flex;
  justify-content:space-around;
  flex-wrap:wrap;
  padding-top:30rpx;
  color: #fff;
}

.msg{
  font-size: 66rpx;
  font-weight: bold;
  text-align: center;
  margin: 10rpx 0;
  color: rgba(255, 255, 255, .9);
  /* transition: all .3s; */
  animation: moveDown 1s forwards;
}


.item{
  margin:0 30rpx 30rpx;
}

.hide {
  visibility: hidden;
  width: 160rpx;
}